{
  "family": "example",
  "containerDefinitions": [
    {
      "name": "parameter-resolver",
      "image": "111111111111.dkr.ecr.ap-southeast-7.amazonaws.com/devops/ssm-resolver:latest",
      "cpu": 0,
      "memory": 64,
      "portMappings": [],
      "essential": false,
      "environment": [
        {
          "name": "PREENV_TEST1",
          "value": "arn:aws:ssm:ap-southeast-7:111111111111:parameter/example/test1"
        },
        {
          "name": "PREENV_TEST2",
          "value": "arn:aws:ssm:ap-southeast-7:111111111111:parameter/example/test2"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "shared-data",
          "containerPath": "/shared"
        }
      ],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "example",
          "awslogs-region": "ap-southeast-7",
          "awslogs-stream-prefix": "example"
        }
      },
      "systemControls": []
    },
    {
      "name": "example",
      "image": "busybox:latest",
      "memory": 384,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": ["/bin/sh", "-c"],
      "command": ["source /shared/resolved-ssm.env && node dist/main.js"],
      "environment": [
        {
          "name": "TEST3",
          "value": "value3"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "shared-data",
          "containerPath": "/shared"
        }
      ],
      "volumesFrom": [],
      "secrets": [],
      "dependsOn": [
        {
          "containerName": "parameter-resolver",
          "condition": "SUCCESS"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "example",
          "awslogs-region": "ap-southeast-7",
          "awslogs-stream-prefix": "example"
        }
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::111111111111:role/example-role",
  "executionRoleArn": "arn:aws:iam::111111111111:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "volumes": [
    {
      "name": "shared-data",
      "host": {}
    }
  ],
  "placementConstraints": []
}
